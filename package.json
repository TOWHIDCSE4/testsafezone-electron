{
  "name": "safezone",
  "version": "2.0.3",
  "description": "SafeZone for Kids",
  "main": "./out/main/index.js",
  "author": "Hamia Edu",
  "homepage": "https://safezone.com.vn",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p electron/tsconfig.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p src/tsconfig.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "deploy": "npm run build && electron-builder --mac --win --linux --publish always",
    "deploy:win": "npm run build && electron-builder --win --publish always",
    "deploy:mac": "npm run build && electron-builder --mac --publish always",
    "deploy:linux": "npm run build && electron-builder --linux --publish always"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@mdi/font": "^7.2.96",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^8.3.0",
    "crc": "^4.3.2",
    "dayjs": "^1.11.9",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "fs-extra": "^11.1.1",
    "madcert": "^2.4.0",
    "mockttp": "^3.7.5",
    "node-machine-id": "^1.1.12",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.15",
    "vue-loading-overlay": "^6.0.3",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@swc/core": "^1.3.56",
    "@types/node": "16.18.21",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.10.0",
    "less": "^4.1.3",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0",
    "vue3-otp-input": "^0.4.1"
  }
}
